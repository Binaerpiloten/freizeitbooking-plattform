{% extends 'BinaerpilotenFreizeitbookingPlattformBundle::backend-base.html.twig' %}

{% block body -%}
    <h1>SEOText</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Heading</th>
                <td>{{ entity.heading }}</td>
            </tr>
            <tr>
                <th>Text</th>
                <td>{{ entity.text }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('seotext') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('seotext_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
